{
    "apps": [{
        "name": "node-nactive",
        "script": "./boot_app.js",
        "instances": "2",
        "merge_logs": true,
        "log_date_format": "YYYY-MM-DD_HH:mm:ss.SSS",
        "exec_mode": "cluster",
        "out_file": "/data/xiaoka/log/node-nactive/nactive-out.log",
        "error_file": "/data/xiaoka/log/node-nactive/nactive-error.log",
        "watch": false,
        "env": {
            "NODE_ENV": "int",
            "NODE_PORT": 8080
        }
    }]
}
